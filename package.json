{
  "name": "commit-release",
  "description": "Commit and tag a release for a conventional changelog project.",
  "version": "1.2.4",
  "author": "Jamie Mason <jamie@foldleft.io> (https://github.com/JamieMason)",
  "bin": {
    "commit-release": "dist/bin.js"
  },
  "bugs": "https://github.com/JamieMason/commit-release/issues",
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "10.0.0",
    "conventional-recommended-version": "1.1.0",
    "fs-extra": "11.1.0",
    "giterator": "0.6.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "1.0.3",
    "@types/chalk": "2.2.0",
    "@types/commander": "2.12.2",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.4.0",
    "@types/node": "14.18.36",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "expect-more-jest": "5.5.0",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/JamieMason/commit-release#readme",
  "issues": "https://github.com/JamieMason/commit-release/issues",
  "keywords": [
    "changelog",
    "commit",
    "conventional",
    "conventional-changelog",
    "deploy",
    "git",
    "log",
    "release",
    "workflow"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "JamieMason/commit-release",
  "resolutions": {
    "chalk": "4.1.2"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc --project .",
    "format": "yarn format:lint && yarn format:source",
    "format:lint": "yarn lint --fix",
    "format:source": "prettier --write .",
    "lint": "eslint --ext .ts .",
    "prepack": "yarn build",
    "test": "jest src test"
  }
}
